{
  "name": "assinador.mpes",
  "version": "2.0.1",
  "description": "Assinador de Documentos do MPES",
  "main": "main.js",
  "author": {
    "name": "Ministério Público do Espírito Santo - MPES",
    "email": "cinf@mpes.mp.br"
  },
  "scripts": {
    "preinstall": "cross-env HOME=~/.electron-gyp npm_config_target=6.0.12 npm_config_arch=x64 npm_config_target_arch=x64 npm_config_disturl=https://electronjs.org/headers npm_config_runtime=electron npm_config_build_from_source=true",
    "dist": "bpm run build && electron-builder",
    "dist:dir": "bpm run dist --dir",
    "release": "npm run build && electron-builder --x64",
    "start": "electron-webpack dev --no-sandbox",
    "build": "electron-webpack",
    "lint": "eslint ./src",
    "docker": "docker run --rm -it -v $PWD:/project -v $PWD/.cache/electron:/root/.cache/electron -v $PWD/.cache/electron-builder:/root/.cache/electron-builder --env ELECTRON_CACHE=\"/root/.cache/electron\" --env ELECTRON_BUILDER_CACHE=\"/root/.cache/electron-builder\" electronuserland/builder:wine",
    "postinstall": "cd node_modules/pkcs11js && cross-env HOME=~/.electron-gyp node-gyp configure build --target=6.0.12 --arch=x64 --dist-url=https://electronjs.org/headers"
  },
  "keywords": [
    "assinador",
    "mpes"
  ],
  "homepage": "https://assinador.mpes.mp.br",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.6",
    "electron-updater": "^4.1.2",
    "electron-webpack": "^2.7.4",
    "electron-webpack-eslint": "^5.0.1",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.0.8",
    "prettier": "^1.18.2",
    "webpack": "^4.41.0"
  },
  "dependencies": {
    "asn1.js-rfc5280": "^3.0.0",
    "auto-launch": "^5.0.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "electron-log": "^3.0.9",
    "express": "^4.17.1",
    "graphene-pk11": "^2.1.3",
    "helmet": "^3.21.1",
    "portfinder": "^1.0.24",
    "source-map-support": "^0.5.13",
    "yup": "^0.27.0"
  }
}
