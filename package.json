{
    "name": "assinador.mpes",
    "version": "0.0.1",
    "description": "Assinador de Documentos do MPES",
    "main": "main.js",
    "scripts": {
        "dist": "yarn build:net && yarn build && electron-builder",
        "dist:dir": "yarn dist --dir",
        "release": "yarn build && electron-builder -w -l",
        "start": "electron-webpack dev --no-sandbox",
        "build": "electron-webpack",
        "net": "node dotnet_helper.js -r",
        "build:net": "node dotnet_helper.js -b -p static/bin"
    },
    "keywords": [
        "assinador",
        "mpes"
    ],
    "author": "Rafael Bubach",
    "license": "MIT",
    "devDependencies": {
        "auto-launch": "^5.0.5",
        "concurrently": "^4.1.0",
        "del": "^4.1.1",
        "electron": "^5.0.6",
        "electron-builder": "^20.40.2",
        "electron-packager": "^13.1.1",
        "electron-webpack": "^2.7.2",
        "gulp": "^4.0.2",
        "gulp-uglify-es": "^1.0.4",
        "minimist": "^1.2.0",
        "webpack": "^4.35.2"
    },
    "dependencies": {
        "electron-log": "^3.0.5",
        "electron-updater": "^4.0.6",
        "wait-on": "^3.2.0"
    }
}
